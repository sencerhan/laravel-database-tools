{
    "name": "sencerhan/laravel-db-tools",
    "description": "Laravel veritabanı tablolarından migration ve seeder dosyaları oluşturmak için araçlar",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sencerhan Taşar",
            "email": "sencerhan76@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/console": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Sencerhan\\LaravelDbTools\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sencerhan\\LaravelDbTools\\LaravelDbToolsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 