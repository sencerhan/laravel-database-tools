{
    "name": "sencerhan/laravel-db-tools",
    "description": "Generate Laravel migration and seeder files from existing database tables. Easily create migrations and seeders with a single command.",
    "type": "library",
    "keywords": [
        "laravel",
        "database",
        "migration",
        "seeder",
        "generator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sencerhan Ta≈üar",
            "email": "sencerhan76@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Sencerhan\\LaravelDbTools\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sencerhan\\LaravelDbTools\\LaravelDbToolsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.0.6"
}